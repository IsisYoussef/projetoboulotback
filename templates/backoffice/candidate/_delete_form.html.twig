<form method="post" action="{{ path('app_back_candidate_delete', {'id': candidate.id}) }}" onsubmit="return confirm('Etes-vous sur(e) de vouloir supprimer ce candidat?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ candidate.id) }}">
    <button class="btn">Supprimer</button>
</form>
